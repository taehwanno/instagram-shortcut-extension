{
  "name": "instagram-shortcut-extension",
  "private": true,
  "version": "1.0.0",
  "description": "instagram-shortcut-extension",
  "main": "index.js",
  "author": "Taehwan Noh <taehwanno.dev@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/taehwanno/instagram-shortcut-extension",
  "scripts": {
    "prestart:firefox": "npm run build",
    "start": "cross-env NODE_ENV=development webpack --watch",
    "start:firefox": "web-ext run --source-dir dist --pref startup.homepage_welcome_url=https://github.com/taehwanno/instagram-shortcut-extension/blob/master/package.json",
    "build": "cross-env NODE_ENV=production webpack",
    "prettier": "prettier --write src/* src/**/* e2e/**/*",
    "prettier:check": "prettier --check src/* src/**/*.ts e2e/**/*.ts",
    "test": "jest --config jest-unit.config.js",
    "test:watch": "npm t -- --watch",
    "e2e": "jest --config jest-e2e.config.js --runInBand",
    "e2e:watch": "npm run e2e -- --watch",
    "package": "web-ext build --source-dir dist --artifacts-dir out --overwrite-dest",
    "release:cws": "webstore upload --source dist --auto-publish",
    "release:amo": "web-ext submit",
    "release": "npm run release:cws && npm run release:amo",
    "ncu": "ncu"
  },
  "devDependencies": {
    "@types/chrome": "0.0.81",
    "@types/jest": "^24.0.9",
    "@types/puppeteer": "^1.12.1",
    "chrome-webstore-upload-cli": "^1.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "lint-staged": "^8.1.4",
    "npm-check-updates": "^2.15.0",
    "prettier": "^1.16.4",
    "puppeteer": "^1.12.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "web-ext": "^3.0.0",
    "webpack": "^4.29.5",
    "webpack-chrome-extension-reloader": "^1.2.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
